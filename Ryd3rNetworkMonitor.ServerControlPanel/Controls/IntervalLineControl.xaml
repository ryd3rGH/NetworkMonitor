<UserControl x:Class="Ryd3rNetworkMonitor.ServerControlPanel.Controls.IntervalLineControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ryd3rNetworkMonitor.ServerControlPanel.Controls"
             mc:Ignorable="d" 
             Height="30" d:DesignWidth="500" VerticalAlignment="Top" HorizontalAlignment="Stretch">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="startLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="0"/>
        <Label x:Name="endLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="1"/>
        <Label x:Name="clicksLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="2"/>
        <Label x:Name="keysLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="3"/>
        <Label x:Name="totalIntervalTimeLbl" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="4"/>
    </Grid>
</UserControl>
